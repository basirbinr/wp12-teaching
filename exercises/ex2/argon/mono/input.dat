memory 16 GB

molecule ar_monomer {
    0 1
    ar      0.00000000000000      0.00000000000000      0.00000000000000

    units bohr
    no_reorient
    symmetry c1
}

# 1. CCSD(T) / aug-cc-pVTZ
core.IO.set_default_namespace("ar_TZ")
set basis aug-cc-pVTZ
set freeze_core true
energy('ccsd(t)')

#  --- clean PSIO before next calculation ---
core.clean()

# 2. MP2 / aug-cc-pVQZ
core.IO.set_default_namespace("ar_QZ")
set basis aug-cc-pVQZ
energy('mp2')
